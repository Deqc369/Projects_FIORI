<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified">
	<Dialog  title="Ver Log" class="sapUiContentPadding" showCloseButton="true" >
		<content>
			<Table items="{path: '/Actividad'}">
				<columns>
					<Column>
					<Text text="Numero Solicitud"/>
					</Column>
					<Column>
					<Text text="Actividad"/>
					</Column>
					<Column>
					<Text text="Usuario"/>
					</Column>
					<Column>
					<Text text="Nivel"/>
					</Column>
					<Column>
					<Text text="Fecha"/>
					</Column>
					<Column>
					<Text text="Hora"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem >
						<cells>
							<Text text="{Nosolicitud}"  />
						</cells>
							<cells>
							<Text text="{StatusSol}" />
						</cells>
							<cells>
							<Text text="{Usuario}"   width="120px"/>
						</cells>
							<cells>
							<Text text="{Nivel}" />
						</cells>
							<cells>
							<Text text="{Fecha}"/>
						</cells>
							<cells>
							<Text text="{Hora}" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<buttons>
			<Button id="btn_cerrar2" text="Cerrar" press="f_cerrar_add" type="Reject"/>
		
		</buttons>
	</Dialog>
</core:FragmentDefinition>